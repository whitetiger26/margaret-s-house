#If Statement
print(a,b,c) if a == b == c else print(a, 'is not equal to', b, "or", c)

print("")

#for loops
for x in range(1):
    print('Hi', J)
    print('Hi', M)    
    print('hi', S)    
    print('Hi', Z)
    
print("")

#While Loop
i = 1
while i<20:
    print(i)
    i += 1



